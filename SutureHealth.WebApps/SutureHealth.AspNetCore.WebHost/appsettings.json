{
  "AWS": {
    "Region": "us-east-1",
    "Logging": {
      "Region": "us-east-1",
      "LogGroup": "suturehealth.webhost",
      "LogLevel": {
        "Default": "Warning",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  },
  "Lambda.Logging": {
    "IncludeNewline": false,
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Information"
    }
  },
  "Logging": {
    "Console": {
      "DisableColors": true
    },
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace"
    }
  },
  "SqlDatabase": {
    "InitialCatalog": {
      "SutureSign": "SutureSignWeb-CI",
      "SutureHealthAPI": "SutureSignAPI-CI"
    }
  },
  "SutureHealth": {
    "Authentication": {
      "DataProtection": {
        "ApplicationName": "SutureSignWeb",
        "TokenLifespanHours": 72
      },
      "Cookie": {
        "Domain": "suturehealth.com",
        "Timeout": "18:00:00"
      },
      "SessionMonitor": {
        "EnableUI": false,
        "IdleTimeout": "00:30:00",
        "WarningTimeout": "00:02:00"
      }
    },
    "DocumentServicesProvider": {
      "TextractTopicArn": "arn:aws:sns:us-east-1:267058832120:TextractProcessing-CI",
      "TextractRoleArn": "arn:aws:iam::267058832120:role/TextractProcessingMessagePublisher"
    },
    "NotificationServicesProvider": {
      "QueueProcessing": true,
      "Providers": {
        "SRFax": {
          "AccessId": "YOUR_SRFAX_ACCESS_ID",
          "AccessPwd": "YOUR_SRFAX_ACCESS_PWD"
        },
        "Twilio": {
          "AccountSid": "YOUR_TWILIO_ACCOUNT_SID",
          "ApiKey": "YOUR_TWILIO_API_KEY",
          "ApiSecret": "YOUR_TWILIO_API_SECRET"
        },
        "CustomerIo": {
          "Preferred": true
        }
      }
    },
    "RequestServicesProvider": {
      "QueueUrl": "https://sqs.us-east-1.amazonaws.com/267058832120/RequestAPI-RequestServiceProvider-CI"
    },
    "S3BinaryStorageBucket": "suturesign-data-ci",
    "S3MemberImagesStorageBucket": "member-organization-images-ci/member-images",
    "S3OrganizationImagesStorageBucket": "member-organization-images-ci/organization-images",
    "Urls": {
      "MemberAccountIndex": "/member/account",
      "MemberAccountLogin": "/identity/account/login",
      "SignIndex": "/request/sign",
      "TermsOfService": "/pdf/SutureHealth%20Terms%20of%20Service.pdf",
      "PrivacyPolicy": "/pdf/SutureHealthPrivacyPolicy_18July2022.pdf"
    }
  },
  "XRay": {
    "DisableXRayTracing": "false",
    "UseRuntimeErrors": "false",
    "CollectSqlQueries": "true"
  }
}
